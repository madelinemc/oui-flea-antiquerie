<h1>all markets</h1>
<div class="grid_container">

    <% @markets.each do |market| %>
        <div class="market_item">
            <p><%= link_to "#{market.name}", market_antiques_path(market) %></p>
            <%= select_market_image(market, false) %>
            <p><%= market.location %></p>
        </div>
    <% end %>

</div>
